    <BuildMacro Include="TouchGFXEnvPath">
      <Value>$(TouchGFXEnvPath)</Value>
    </BuildMacro>
    <BuildMacro Include="TextConverterOptions">
      <Value>$(TextConverterOptions)</Value>
    </BuildMacro>
    <BuildMacro Include="ApplicationRoot">
      <Value>$(ApplicationRoot)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
