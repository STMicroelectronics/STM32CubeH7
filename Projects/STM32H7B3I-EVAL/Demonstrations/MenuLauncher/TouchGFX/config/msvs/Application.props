<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <ImportGroup Label="PropertySheets"/>
  <PropertyGroup Label="UserMacros">
    <UseBPP>24</UseBPP>
    <TouchGFXReleasePath>..\..\..\Middlewares\ST\TouchGFX\touchgfx</TouchGFXReleasePath>
    <TouchGFXEnvPath>D:\TouchGFX\4.12.3\env</TouchGFXEnvPath>
    <TextConverterOptions>remap A4 BPP24</TextConverterOptions>
    <ApplicationRoot>..\..</ApplicationRoot>
  </PropertyGroup>
  <PropertyGroup/>
